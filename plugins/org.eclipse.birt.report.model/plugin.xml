<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.birt.report.model"
   name="BIRT Model"
   version="1.0.0"
   provider-name="Eclipse.org"
>
   <runtime>
      <library name="model.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-codec-1.3.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.birt.core"/>
   </requires>
   <extension-point id="reportitem" name="reportitem" schema="schema/element.exsd"/>   
</plugin>